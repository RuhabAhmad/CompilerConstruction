package main;
import scanner.Yylex;
import tokens.Token;
import tokens.TokenType;
import java.io.FileReader;

public class Main {
    public static void main(String[] args) throws Exception {
        Yylex lexer = new Yylex(new FileReader("tests/test5.nexus"));

        Token token;

        while ((token = scanner.getNextToken()) != null) {
            System.out.println(token);
        }

        System.out.println("\nTotal Tokens: " + lexer.getTokenCount());
        System.out.println("Lines Processed: " + lexer.getLineCount());
        System.out.println("Comments Removed: " + lexer.getCommentCount());
    }
}
